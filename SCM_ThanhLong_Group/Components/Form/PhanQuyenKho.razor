@page "/phanquyenkho"
@inject Kho_Service khoService
@inject IToastService toastService

<TelerikComboBox @bind-Value="@khoId" Data="@lstKho" TextField="TenKho" ValueField="Auto_ID" Placeholder="Chọn Kho"></TelerikComboBox>


@code {
    List<Kho> lstKho=new List<Kho>();
    private int khoId;

    protected override async Task OnInitializedAsync()
    {
        lstKho = await khoService.getAllData();
    }

}
